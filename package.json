    "@babel/runtime": "^7.15.4",
    "@types/glob": "^7.1.4",
    "@types/jest": "^27.0.2",
    "@types/lz-string": "^1.3.34",
    "@types/node": "^16.10.1",
    "@types/webpack": "^5.28.0",
